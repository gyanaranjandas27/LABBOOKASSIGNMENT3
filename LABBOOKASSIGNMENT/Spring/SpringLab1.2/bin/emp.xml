<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
	xmlns:context="http://www.springframework.org/schema/context"
	xsi:schemaLocation="http://www.springframework.org/schema/beans 
      http://www.springframework.org/schema/beans/spring-beans-4.3.xsd
      http://www.springframework.org/schema/context 
      http://www.springframework.org/schema/context/spring-context-4.3.xsd">

  <bean id="emp" class="com.cg.iter.Employee">      
      <property name="empAge" value="25"/>
      <property name="empId" value="2055"/>
      <property name="empName" value="Gyana"/>
      <property name="salary" value="88888"/>
      <property name="SBU" ref="add"/>
  </bean>
  <bean id="add" class="com.cg.iter.SBU">
      <property name="sbuCode" value="CAPG-RM"/>
      <property name="sbuHead" value="Amar Das"/>
      <property name="sbuName" value="SAP SERVICE"/>
  </bean>
</beans>
